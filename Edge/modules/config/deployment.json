            "generatemessages": {
              "version": "1.0",
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "containerregistryju.azurecr.io/generatemessages:1.8.3",
                "createOptions": "{\"HostConfig\": {\"Privileged\": true}}"
