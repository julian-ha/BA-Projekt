          },
          "modules": {
            "emitmessages": {
              "version": "1.0",
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "containerregistryju.azurecr.io/emitmessages:1.0.0",
                "createOptions": "{}"
