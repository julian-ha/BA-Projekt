{
    "modulesContent": {
      "$edgeAgent": {
        "properties.desired": {
          "schemaVersion": "1.0",
          "runtime": {
            "type": "docker",
            "settings": {
              "minDockerVersion": "v1.25",
              "loggingOptions": "",
              "registryCredentials": {
                "docker": {
                  "username": "containerRegistryju",
                  "password": "e2wWPw4pMnr/Zb1HB9MXzqxa5hJRaafz",
                  "address": "containerregistryju.azurecr.io"
                }
              }
            }
          },
          "systemModules": {
            "edgeAgent": {
              "type": "docker",
              "settings": {
                "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
                "createOptions": "{}"
              }
            },
            "edgeHub": {
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
                "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"5671/tcp\":[{\"HostPort\":\"5671\"}],\"8883/tcp\":[{\"HostPort\":\"8883\"}],\"443/tcp\":[{\"HostPort\":\"443\"}]}}}"
              }
            }
          },
          "modules": {
            "lightsmodule": {
              "version": "1.0",
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "containerregistryju.azurecr.io/lightsmodulearm32v7:3.0.0",
                "createOptions": "{\"HostConfig\": {\"Privileged\": true}}"
              },
              "env": {
                "pinNumberGreen": {
                  "value": 36
                },
                "pinNumberYellow": {
                  "value": 38
                },
                "pinNumberRed": {
                  "value": 40
                }
              }
            },
            "readingsmodule": {
              "version": "1.0",
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "containerregistryju.azurecr.io/readingsmodulepython:1.0.28",
                "createOptions": "{\"HostConfig\": {\"Privileged\": true}}"
              },
              "env": {
                "co2ThresholdRed": {
                  "value": -1
                },
                "co2ThresholdYellow": {
                  "value": -1
                }
              }
            },
            "printermodule": {
              "version": "1.0",
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "containerregistryju.azurecr.io/printermodule:1.0.2",
                "createOptions": "{\"HostConfig\": {\"Privileged\": true}}"
              },
              "env": {
                "co2ThresholdRed": {
                  "value": -1
                },
                "co2ThresholdYellow": {
                  "value": -1
                }
              }
            }
            
          }
        }
      },
      "$edgeHub": {
        "properties.desired": {
          "schemaVersion": "1.0",
          "routes": {
            "EdgeModuleToIoTHub": "FROM /messages/modules/readingsmodule/outputs/cloudMessage INTO $upstream",
            "printerModule": "FROM /messages/modules/printermodule/outputs/cloudMessage INTO $upstream",
            "readingsModuleToLightsModule": "FROM /messages/modules/readingsmodule/outputs/thresholdData INTO BrokeredEndpoint(\"/modules/lightsmodule/inputs/generatedMessage\")"
          },
          "storeAndForwardConfiguration": {
            "timeToLiveSecs": 7200
          }
        }
      }
    }
  }
